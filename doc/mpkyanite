    <span class="ruby-keyword">end</span>
    
  <span class="ruby-keyword">when</span> <span class="ruby-value">:all</span>
    <span class="ruby-keyword">unless</span> ( <span class="ruby-keyword">self</span>.<span class="ruby-identifier">kind_of?</span>(<span class="ruby-constant">Hash</span>) <span class="ruby-operator">||</span> <span class="ruby-keyword">self</span>.<span class="ruby-identifier">kind_of?</span>(<span class="ruby-constant">Hashery</span><span class="ruby-operator">::</span><span class="ruby-constant">Dictionary</span>) )
      <span class="ruby-identifier">c</span> = <span class="ruby-keyword">self</span>.<span class="ruby-identifier">collect</span> {<span class="ruby-operator">|</span><span class="ruby-identifier">e</span><span class="ruby-operator">|</span> <span class="ruby-identifier">e</span>.<span class="ruby-identifier">class</span>}
    <span class="ruby-keyword">else</span>
      <span class="ruby-identifier">c</span> = <span class="ruby-keyword">self</span>.<span class="ruby-identifier">collect</span> {<span class="ruby-operator">|</span> <span class="ruby-identifier">key</span>, <span class="ruby-identifier">value</span> <span class="ruby-operator">|</span> <span class="ruby-identifier">value</span>.<span class="ruby-identifier">class</span>}      
    <span class="ruby-keyword">end</span> 
    <span class="ruby-identifier">c</span> = <span class="ruby-identifier">c</span> <span class="ruby-operator">-</span> [<span class="ruby-constant">NilClass</span>]  <span class="ruby-keyword">if</span> <span class="ruby-identifier">ignore_nil</span>
    <span class="ruby-identifier">c</span>.<span class="ruby-identifier">uniq!</span>  
