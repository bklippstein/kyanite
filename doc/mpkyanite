    ( ( <span class="ruby-keyword">self</span>.<span class="ruby-identifier">to_s</span> ) <span class="ruby-operator">&lt;=&gt;</span> ( <span class="ruby-identifier">other</span>.<span class="ruby-identifier">to_s</span> ) )
