    <span class="ruby-keyword">return</span> <span class="ruby-constant">Object</span>

  <span class="ruby-keyword">else</span> <span class="ruby-comment"># case precision</span>
    <span class="ruby-identifier">raise</span> <span class="ruby-constant">ArgumentError</span>, <span class="ruby-string">':precision should be 1, 2 or :all'</span>
  <span class="ruby-keyword">end</span> <span class="ruby-comment">#case</span>
  
<span class="ruby-keyword">end</span></pre>
