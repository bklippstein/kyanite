<span class="ruby-keyword">def</span> <span class="ruby-identifier">mean</span>
  <span class="ruby-keyword">self</span>.<span class="ruby-identifier">inject</span>(<span class="ruby-value">0.0</span>) { <span class="ruby-operator">|</span><span class="ruby-identifier">sum</span>, <span class="ruby-identifier">i</span> <span class="ruby-operator">|</span> <span class="ruby-identifier">sum</span> <span class="ruby-operator">+=</span> <span class="ruby-identifier">i</span> } <span class="ruby-operator">/</span> <span class="ruby-keyword">self</span>.<span class="ruby-identifier">length</span>.<span class="ruby-identifier">to_f</span> 
<span class="ruby-keyword">end</span></pre>
          </div><!-- mean-source -->
          
        </div>

        
