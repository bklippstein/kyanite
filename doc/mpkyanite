  <span class="ruby-keyword">self</span>.<span class="ruby-identifier">mask</span>( <span class="ruby-value">:level_start</span> =<span class="ruby-operator">&gt;</span> <span class="ruby-value">0</span>,
             <span class="ruby-value">:pattern</span> =<span class="ruby-operator">&gt;</span> <span class="ruby-identifier">pattern</span>,
             <span class="ruby-value">:param_level</span> =<span class="ruby-operator">&gt;</span> <span class="ruby-keyword">true</span> ) { <span class="ruby-operator">|</span><span class="ruby-identifier">s</span>,<span class="ruby-identifier">l</span><span class="ruby-operator">|</span> 
             <span class="ruby-keyword">if</span> <span class="ruby-identifier">l</span> <span class="ruby-operator">&gt;</span> <span class="ruby-identifier">result</span>
