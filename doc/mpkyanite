  <span class="ruby-keyword">self</span>.<span class="ruby-identifier">inject</span>(<span class="ruby-value">1.0</span>) { <span class="ruby-operator">|</span><span class="ruby-identifier">p</span>, <span class="ruby-identifier">i</span> <span class="ruby-operator">|</span> <span class="ruby-identifier">p</span> <span class="ruby-operator">*=</span> <span class="ruby-identifier">i</span> }  
<span class="ruby-keyword">end</span></pre>
          </div><!-- prd-source -->
          
